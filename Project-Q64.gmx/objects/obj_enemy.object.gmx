//Vertical Collision
if (place_meeting(x,y+vertical_speed,obj_wall)){
    while(!place_meeting(x,y+sign(vertical_speed),obj_wall))    {
        y += sign(vertical_speed);
    }
    vertical_speed = 0;

}
y += vertical_speed;

if( can_attack &gt; 0 ) {
    can_attack -= 1;
}

//Enemy Collision
if (place_meeting(x,y,obj_link)){
    if (obj_link.y &lt; y-80)    {
        with (obj_link) vertical_speed = -jump_speed;
        instance_destroy();
    } else {
        if( can_attack == 0 ) {
            player_hp -= 2;
            can_attack = 45;
        }
        //game_restart();
    }
}

